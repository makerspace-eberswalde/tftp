DISPLAY boot.txt
TIMEOUT 10000
PROMPT 0
DEFAULT _display/vesamenu.c32

MENU BEGIN Ubuntu Unity (Standard Ubuntu)
	LABEL Previous
	MENU LABEL Previous Menu
	TEXT HELP
	Return to previous menu
	ENDTEXT
	MENU EXIT
	MENU SEPARATOR
	MENU INCLUDE pxelinux.cfg/ubuntu-unity
MENU END
