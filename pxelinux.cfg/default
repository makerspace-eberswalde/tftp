DISPLAY boot.txt
TIMEOUT 10000
PROMPT 0
DEFAULT _display/vesamenu.c32

MENU BEGIN Ubuntu UNITY 14.04 (manuelle Installation)
	LABEL Previous
	MENU LABEL Previous Menu
	TEXT HELP
	Return to previous menu
	ENDTEXT
	MENU EXIT
	MENU SEPARATOR
	MENU INCLUDE menus/ubuntu-unity-14.04-amd64/menu.cfg
MENU END
